# backend/.env

# Deploy tier: dev | production
APP_ENV=dev

# Log verbosity policy
LOG_LEVEL=INFO

# CORS: origins allowed to call the API in dev
# CSV, no spaces; edit here per environment, not in code
# Includes both hostnames to avoid mismatch
# If frontend does not use port 5173 on Vite, change it here
CORS_ORIGINS=["http://localhost:5173","http://127.0.0.1:5173"]